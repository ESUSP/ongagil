<%- model_class = Beneficiary -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:projeto) %>:</strong></dt>
  <dd><%= @beneficiary.projeto %></dd>
  <dt><strong><%= model_class.human_attribute_name(:nome) %>:</strong></dt>
  <dd><%= @beneficiary.nome %></dd>
  <dt><strong><%= model_class.human_attribute_name(:dataNasc) %>:</strong></dt>
  <dd><%= @beneficiary.dataNasc %></dd>
  <dt><strong><%= model_class.human_attribute_name(:estadoCivil) %>:</strong></dt>
  <dd><%= @beneficiary.estadoCivil %></dd>
  <dt><strong><%= model_class.human_attribute_name(:rg) %>:</strong></dt>
  <dd><%= @beneficiary.rg %></dd>
  <dt><strong><%= model_class.human_attribute_name(:cpf) %>:</strong></dt>
  <dd><%= @beneficiary.cpf %></dd>
  <dt><strong><%= model_class.human_attribute_name(:telefone) %>:</strong></dt>
  <dd><%= @beneficiary.telefone %></dd>
  <dt><strong><%= model_class.human_attribute_name(:endereco) %>:</strong></dt>
  <dd><%= @beneficiary.endereco %></dd>
  <dt><strong><%= model_class.human_attribute_name(:bairro) %>:</strong></dt>
  <dd><%= @beneficiary.bairro %></dd>
  <dt><strong><%= model_class.human_attribute_name(:estado) %>:</strong></dt>
  <dd><%= @beneficiary.estado %></dd>
  <dt><strong><%= model_class.human_attribute_name(:pais) %>:</strong></dt>
  <dd><%= @beneficiary.pais %></dd>
  <dt><strong><%= model_class.human_attribute_name(:cep) %>:</strong></dt>
  <dd><%= @beneficiary.cep %></dd>
  <dt><strong><%= model_class.human_attribute_name(:menor) %>:</strong></dt>
  <dd><%= @beneficiary.menor %></dd>
  <dt><strong><%= model_class.human_attribute_name(:responsavel) %>:</strong></dt>
  <dd><%= @beneficiary.responsavel %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              beneficiaries_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_beneficiary_path(@beneficiary), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              beneficiary_path(@beneficiary),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
