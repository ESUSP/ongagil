<%- model_class = Recebimento -%>
<br>
<br>
<br>
  <div class="page-header">
    <h1>
      RECEBIMENTO
    </h1>
  </div>
  <br>
  <div class="table-responsive">
    <table id="tblrecebimentos" class="table table-bor table-striped table-hover" cellspacing="0" width="100%">
      <thead>
        <tr>
          <th>
            <%= model_class.human_attribute_name(:id) %>
          </th>
          <th>
            Tipo de Recebimento
          </th>
          <th>
            Data de Recebimento
          </th>
          <th>
            Local
          </th>
          <th>
            Unidade
          </th>
          <th>
            Projeto
          </th>
          <th>
            Responsável
          </th>
          <th>
            Beneficiários
          </th>
          
          <th>
            <%=t '.actions', :default => t("helpers.actions") %>
          </th>
        </tr>
      </thead>
      <tbody>
        <% @recebimentos.each do |recebimento| %>
          <tr>
            <td>
              <%= link_to recebimento.id, recebimento_path(recebimento) %>
            </td>
            <td>
              <%= recebimento.tipoRecebimento %>
            </td>
            <td>
              <%= recebimento.dataRecebimento %>
            </td>
            <td>
              <%= recebimento.local %>
            </td>
            <td>
              <%= recebimento.unidade %>
            </td>
            <td>
              <%= recebimento.projeto %>
            </td>
            <td>
              <%= recebimento.responsavel %>
            </td>
            <td>
              <%= recebimento.beneficiarios %>
            </td>
            

            <td>
              <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_recebimento_path(recebimento), :class => 'btn btn-default btn-xs' %>
                <%= link_to t('.show', :default => t("helpers.links.show")),
                      recebimento_path(recebimento), :class => 'btn btn-default btn-xs' %>
                  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      recebimento_path(recebimento),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-xs btn-danger' %>
            </td>
          </tr>
          <% end %>
      </tbody>
    </table>
  </div>
  <%= link_to t('.new', :default => t("helpers.links.new")),
            new_recebimento_path,
            :class => 'btn btn-success btn-lg' %>





