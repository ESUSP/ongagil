<%- model_class = Recebimento -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:Tiporecebimento) %>:</strong></dt>
  <dd><%= @recebimento.tipoRecebimento %></dd>
  <dt><strong><%= model_class.human_attribute_name(:dataRecebimento) %>:</strong></dt>
  <dd><%= @recebimento.dataRecebimento %></dd>
  <dt><strong><%= model_class.human_attribute_name(:local) %>:</strong></dt>
  <dd><%= @recebimento.local %></dd>
  <dt><strong><%= model_class.human_attribute_name(:unidade) %>:</strong></dt>
  <dd><%= @recebimento.unidade %></dd>
  <dt><strong><%= model_class.human_attribute_name(:projeto) %>:</strong></dt>
  <dd><%= @recebimento.projeto %></dd>
  <dt><strong><%= model_class.human_attribute_name(:responsavel) %>:</strong></dt>
  <dd><%= @recebimento.responsavel %></dd>
  <dt><strong><%= model_class.human_attribute_name(:beneficiarios) %>:</strong></dt>
  <dd><%= @recebimento.beneficiarios %></dd>
</dl>
 <br>
 <br>
 <br>
 <br>

<%= link_to t('.back', :default => t("helpers.links.back")),
              recebimentos_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_recebimento_path(@recebimento), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              recebimento_path(@recebimento),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
             
