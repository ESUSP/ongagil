<div class="row">
  <div class="col-md-12">
    <p id="notice"><%= notice %></p>    
  </div>
  <div class="col-md-8">
    <h1>Listagem de Projetos</h1>    
  </div>
  <div class="col-md-4">
    <span class="pull-right">
    <%= link_to 'Cadastrar Novo Projeto', new_projeto_path, class:"btn btn-success" %>  
    </span>  
  </div>
</div>
<br>
<div class="row">
  <div class="col-md-12">



    <table class="table ">
      <thead>
        <tr>


          <th>Projeto</th>
          <th>Descrição</th>
          <th>Status</th>
          <th>Tipo</th>
          <th>Area</th>
          <th>Início</th>
          <th>Termino</th>      
          <th>ONG</th>      
          <th>Responsável</th>      
          <th colspan="3"></th>
        </tr>
      </thead>

      <tbody>
        <% @projetos.each do |projeto| %>
          <tr>


            <td><%= projeto.Projeto %></td>
            <td><%= projeto.DescricaoProjeto %></td>
            <td><%= projeto.Status %></td>
            <td><%= projeto.Tipo %></td>
            <td><%= projeto.Area %></td>
            <td><%= projeto.DataInicio %></td>
            <td><%= projeto.DataFim %></td>        
            <td><%= projeto.Ong %></td>        
            <td><%= projeto.RespNome %></td>

            <td><%= link_to 'Show', projeto, class: 'btn btn-primary' %></td>
            <td><%= link_to 'Edit', edit_projeto_path(projeto), class: 'btn btn-warning' %></td>
            <td><%= link_to 'Destroy', projeto, method: :delete, data: { confirm: 'Are you sure?' }, class: 'btn btn-danger' %></td>
          </tr>
        <% end %>
      </tbody>
    </table>

  </div>
  
</div>
  

